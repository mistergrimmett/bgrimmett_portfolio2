            <div class="row">
                <div class="col-md-6">
                    <div class="row">
                        <p>&copy; 2017 @Html.ActionLink("Brandon Grimmett", "Index", "Home")</p>
                    </div>
                    <div class="row">
                        <i class="icon-location-pin">Charlotte, NC</i>
                    </div>
                </div>
                <div class="text-center">
                    <div class="col-md-6">
                        <div class="row">
                            <i class="icon-email"> <a href="@Url.Action("Contact", "Home")">mistergrimmett1127@gmail.com</a></i>
                        </div>
                        <div class="row">
                            <i class="icon-phone2"><a href="tel:7043146774"> (704)314-6774</a></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="gototop js-top">
        <a href="#" class="js-gotop"><i class="icon-arrow-up22"></i></a>
    </div>

    <!-- jQuery -->
    <script src="~/template/js/jquery.min.js"></script>
