    <title>Profile &mdash; Brandon Keith Grimmett</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Brandon Grimmett Full Stack Web Developer" />
    <meta name="keywords" content="C#, ASP.NET, MVC, JavaScript, HTML, CSS, Full Stack, Web Developer" />
